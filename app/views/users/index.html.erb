<div class="container">
  <div class="row">
    <div class="col-lg-4">
        <%= render "users/list", book_list: @book_list, user: @user %>

    </div>
    <div class="col-lg-8">
<table class="table table-striped">

 <tr>
   <th>image</th>
   <th>name</th>
   <th></th>
 </tr><h1>Users</h1>


<% @users.each do |user| %>
<tr>
   <th><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></th>
   <th>
    <%= user.name %>
   </th>
   <th><%= link_to "Show", user_path(user.id) %></th>
  </tr>

<% end %>
</table>
</div>
 </div>
</div>
